build:
	bison -t -v -d parser.y
	flex lex.l
	gcc -Wall -c ts.c -o ts.o
	gcc -Wall -c main.c -o main.o
	gcc -Wall -c asm_instruction.c -o asm_instruction.o
	gcc -Wall -g -o test main.o ts.o
	gcc -o testpars parser.tab.c lex.yy.c 

clean:
	rm test parser.tab.c lex.yy.c parser.tab.h parser.output

test:
	./test